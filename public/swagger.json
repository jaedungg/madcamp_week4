{
  "openapi": "3.0.0",
  "info": {
    "title": "프롬 API",
    "version": "1.0.0",
    "description": "API for creating and fetching documents"
  },
  "paths": {
    "/api/signup": {
      "post": {
        "summary": "사용자 회원가입",
        "tags": ["Auth"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "example": "홍길동"
                  },
                  "email": {
                    "type": "string",
                    "format": "email",
                    "example": "test@example.com"
                  },
                  "password": {
                    "type": "string",
                    "format": "password",
                    "example": "test123!"
                  }
                },
                "required": ["name", "email", "password"]
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "회원가입 성공",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "user": {
                      "type": "object",
                      "properties": {
                        "id": {
                          "type": "integer",
                          "example": 1
                        },
                        "name": {
                          "type": "string",
                          "example": "홍길동"
                        },
                        "email": {
                          "type": "string",
                          "format": "email",
                          "example": "test@example.com"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "409": {
            "description": "이미 존재하는 이메일입니다."
          },
          "500": {
            "description": "회원가입 실패"
          }
        }
      }
    },
    "/api/login": {
      "post": {
        "summary": "사용자 로그인",
        "tags": ["Auth"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string",
                    "format": "email",
                    "example": "test@example.com"
                  },
                  "password": {
                    "type": "string",
                    "format": "password",
                    "example": "mySecret123!"
                  }
                },
                "required": ["email", "password"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "로그인 성공",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "integer",
                      "example": 1
                    },
                    "email": {
                      "type": "string",
                      "format": "email",
                      "example": "test@example.com"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "이메일 또는 비밀번호가 올바르지 않습니다."
          },
          "500": {
            "description": "서버 오류"
          }
        }
      }
    },
    "/api/users/{id}": {
      "get": {
        "summary": "유저 정보 업데이트",
        "tags": ["Users"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "불러올 유저의 ID",
            "schema": {
              "type": "string",
              "example": "a888fe3f-8a7e-4069-98e2-173c8c74c645"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "유저 정보 불러오기 성공"
          },
          "404": {
            "description": "해당 유저를 찾을 수 없음"
          },
          "500": {
            "description": "서버 오류"
          }
        }
      },
      "put": {
        "summary": "유저 정보 업데이트",
        "tags": ["Users"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "업데이트할 유저의 ID",
            "schema": {
              "type": "string",
              "example": "a888fe3f-8a7e-4069-98e2-173c8c74c645"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "example": "wogus"
                  },
                  "password": {
                    "type": "string",
                    "minLength": 6,
                    "example": "test123!"
                  },
                  "profile_image": {
                    "type": "string",
                    "example": "/uploads/newprofile.png"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "유저 정보 업데이트 성공",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": { "type": "boolean", "example": true },
                    "user": {
                      "type": "object",
                      "properties": {
                        "id": { "type": "string", "example": "f8a1b9c4-1234-4df0-8a4e-abcdef123456" },
                        "name": { "type": "string", "example": "홍길동" },
                        "email": { "type": "string", "example": "hong@example.com" },
                        "profile_image": { "type": "string", "example": "/uploads/profile1.jpg" }
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "잘못된 입력 또는 업데이트할 정보 없음"
          },
          "404": {
            "description": "해당 유저를 찾을 수 없음"
          },
          "500": {
            "description": "서버 오류"
          }
        }
      }
    },
    "/api/users/{id}/password": {
      "put": {
        "summary": "유저 비밀번호 변경",
        "tags": ["Users"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "비밀번호를 변경할 유저의 ID",
            "schema": {
              "type": "string",
              "example": "a888fe3f-8a7e-4069-98e2-173c8c74c645"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "currentPassword": {
                    "type": "string",
                    "minLength": 6,
                    "example": ""
                  },
                  "newPassword": {
                    "type": "string",
                    "minLength": 6,
                    "example": ""
                  }
                },
                "required": ["currentPassword", "newPassword"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "비밀번호 변경 성공",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": { "type": "boolean", "example": true },
                    "message": { "type": "string", "example": "비밀번호가 변경되었습니다." }
                  }
                }
              }
            }
          },
          "400": {
            "description": "입력값이 유효하지 않음"
          },
          "403": {
            "description": "현재 비밀번호 불일치"
          },
          "404": {
            "description": "해당 유저를 찾을 수 없음"
          },
          "500": {
            "description": "서버 오류"
          }
        }
      }
    },
    "/api/documents": {
      "post": {
        "summary": "Create a document",
        "tags": ["Documents CRUD"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/DocumentInput"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Document created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Document"
                }
              }
            }
          },
          "400": {
            "description": "Missing required fields"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      },
      "get": {
        "summary": "Get list of documents",
        "tags": ["Documents CRUD"],
        "parameters": [
          { "name": "page", "in": "query", "schema": { "type": "integer", "default": 1 } },
          { "name": "limit", "in": "query", "schema": { "type": "integer", "default": 10 } },
          { "name": "category", "in": "query", "schema": { "type": "string" } },
          { "name": "status", "in": "query", "schema": { "type": "string" } },
          { "name": "search", "in": "query", "schema": { "type": "string" } },
          { "name": "sortBy", "in": "query", "schema": { "type": "string", "enum": ["created_at", "updated_at", "title", "word_count"] } },
          { "name": "sortOrder", "in": "query", "schema": { "type": "string", "enum": ["asc", "desc"] } },
          { "name": "favoritesOnly", "in": "query", "schema": { "type": "boolean" } },
          { "name": "user_id", "in": "query", "schema": { "type": "string", "format": "uuid" } }
        ],
        "responses": {
          "200": {
            "description": "List of documents",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DocumentListResponse"
                }
              }
            }
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/api/documents/bulk-delete": {
      "post": {
        "summary": "Delete multiple documents",
        "tags": ["Documents management"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "documentIds": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  }
                },
                "required": ["documentIds"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Multiple documents deleted",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "deletedCount": {
                      "type": "number"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "100개 제한을 초과하였거나 잘못된 문서 ID 형식입니다."
          },
          "404": {
            "description": "요청한 문서 중 일부를 찾을 수 없습니다."
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/api/documents/{id}": {
      "get": {
        "summary": "Get a document by ID",
        "tags": ["Documents CRUD"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": { "type": "string", "format": "uuid" }
          }
        ],
        "responses": {
          "200": {
            "description": "Document retrieved",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": { "type": "boolean" },
                    "document": { "$ref": "#/components/schemas/Document" }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Document not found"
          }
        }
      },
      "put": {
        "summary": "Update a document",
        "tags": ["Documents CRUD"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": { "type": "string", "format": "uuid" }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "title": { "type": "string" },
                  "content": { "type": "string" },
                  "category": { "type": "string" },
                  "status": { "type": "string" },
                  "tags": {
                    "type": "array",
                    "items": { "type": "string" }
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Document updated",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": { "type": "boolean" },
                    "document": { "$ref": "#/components/schemas/Document" }
                  }
                }
              }
            }
          },
          "400": { "description": "Invalid input" },
          "404": { "description": "Document not found" }
        }
      },
      "delete": {
        "summary": "Delete a document",
        "tags": ["Documents CRUD"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": { "type": "string", "format": "uuid" }
          }
        ],
        "responses": {
          "200": {
            "description": "Document deleted",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": { "type": "boolean" }
                  }
                }
              }
            }
          },
          "404": { "description": "Document not found" }
        }
      }
    },
    "/api/documents/{id}/duplicate": {
      "post": {
        "summary": "Duplicate a document",
        "tags": ["Documents management"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": { "type": "string", "format": "uuid" }
          }
        ],
        "responses": {
          "201": {
            "description": "Document duplicated",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Document"
                }
              }
            }
          },
          "400": {
            "description": "Missing required fields"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/api/documents/{id}/favorite": {
      "put": {
        "summary": "Update is_favorte",
        "tags": ["Documents management"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": { "type": "string", "format": "uuid" }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "is_favorite": { "type": "boolean" }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Updated is_favorite",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Document"
                }
              }
            }
          },
          "400": {
            "description": "Missing required fields"
          },
          "404": {
            "description": "Document not found"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/api/documents/recent": {
      "get": {
        "summary": "사용자의 최근 문서 조회",
        "tags": ["Recent Documents"],
        "parameters": [
          {
            "name": "user_id",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "조회할 사용자의 ID"
          },
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "default": 10
            },
            "description": "조회할 문서 수 (기본값: 10)"
          }
        ],
        "responses": {
          "200": {
            "description": "최근 문서 조회 성공",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean",
                      "example": true
                    },
                    "documents": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": { "type": "string", "example": "doc_123" },
                          "title": { "type": "string", "example": "AI Report" },
                          "content": { "type": "string", "example": "본문 내용..." },
                          "excerpt": { "type": "string", "example": "요약 내용..." },
                          "wordCount": { "type": "integer", "example": 1200 },
                          "category": { "type": "string", "example": "Work" },
                          "tags": {
                            "type": "array",
                            "items": { "type": "string" },
                            "example": ["AI", "Research"]
                          },
                          "isFavorite": { "type": "boolean", "example": false },
                          "createdAt": { "type": "string", "format": "date-time", "example": "2024-06-01T10:00:00Z" },
                          "updatedAt": { "type": "string", "format": "date-time", "example": "2024-07-01T10:00:00Z" },
                          "lastModifiedAt": { "type": "string", "format": "date-time", "example": "2024-07-15T10:00:00Z" },
                          "status": { "type": "string", "example": "draft" },
                          "aiRequestsUsed": { "type": "integer", "example": 3 },
                          "lastAccessedAt": { "type": "string", "format": "date-time", "example": "2024-07-27T12:30:00Z" },
                          "timeSpent": { "type": "number", "example": 12.5, "description": "분 단위 시간" }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "필수 파라미터 누락 (user_id)"
          },
          "500": {
            "description": "서버 내부 오류"
          }
        }
      }
    },
    "/api/documents/{id}/access": {
      "post": {
        "summary": "Create a document access log",
        "tags": ["Recent Documents"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": { "type": "string", "format": "uuid" }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "user_id": {
                    "type": "string",
                    "format": "uuid"
                  },
                  "time_spent": {
                    "type": "integer",
                    "nullable": true
                  }
                },
                "required": ["user_id"]
              },
              "example": {
                "user_id": "a888fe3f-8a7e-4069-98e2-173c8c74c645",
                "time_spent": 120
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Document access log created"
          },
          "404": {
            "description": "Document not found"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/api/documents/stats": {
      "get": {
        "summary": "문서 통계 조회",
        "tags": ["Documents Analytics"],
        "parameters": [
          {
            "name": "user_id",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            },
            "description": "특정 사용자의 문서 통계를 조회하려면 사용자 ID를 전달합니다."
          }
        ],
        "responses": {
          "200": {
            "description": "문서 통계 조회 성공",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": { "type": "boolean", "example": true },
                    "stats": {
                      "type": "object",
                      "properties": {
                        "totalDocuments": { "type": "integer", "example": 120 },
                        "totalWords": { "type": "integer", "example": 48000 },
                        "avgWordsPerDocument": { "type": "number", "example": 400.0 },
                        "documentsByCategory": {
                          "type": "object",
                          "properties": {
                            "business-email": { "type": "integer", "example": 30 },
                            "personal-letter": { "type": "integer", "example": 25 },
                            "thank-you": { "type": "integer", "example": 15 },
                            "apology-message": { "type": "integer", "example": 10 },
                            "casual-message": { "type": "integer", "example": 40 }
                          }
                        },
                        "documentsByStatus": {
                          "type": "object",
                          "properties": {
                            "draft": { "type": "integer", "example": 50 },
                            "published": { "type": "integer", "example": 60 },
                            "archived": { "type": "integer", "example": 10 }
                          }
                        },
                        "weeklyActivity": {
                          "type": "array",
                          "items": {
                            "type": "object",
                            "properties": {
                              "date": { "type": "string", "format": "date", "example": "2025-07-21" },
                              "count": { "type": "integer", "example": 8 }
                            }
                          }
                        },
                        "aiUsageStats": {
                          "type": "object",
                          "properties": {
                            "totalRequests": { "type": "integer", "example": 150 },
                            "mostUsedFeature": { "type": "string", "example": "generate" },
                            "costEstimate": { "type": "number", "example": 1.5 }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "문서 통계 조회 실패",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": { "type": "boolean", "example": false },
                    "error": { "type": "string", "example": "문서 통계를 가져오는데 실패했습니다." }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/documents/export": {
      "get": {
        "summary": "문서 내보내기 API 정보 조회",
        "tags": ["Documents Import/Export"],
        "responses": {
          "200": {
            "description": "API 설명 반환",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "프롬 문서 내보내기 API"
                    },
                    "version": {
                      "type": "string",
                      "example": "1.0.0"
                    },
                    "methods": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      },
                      "example": ["POST"]
                    },
                    "description": {
                      "type": "string",
